3. SearchBar.js

import React from 'react'
import './SearchBar.css'

const SearchBar= () =>{

    const SearchValue ='just a msg'
    const handleInputChange =()=>{
        alert("Don't type")
    }

    return<div>
        <input type='text' value={SearchValue} onChange={handleInputChange}></input>
        </div>
}

